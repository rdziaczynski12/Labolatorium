
@{
    ViewData["Title"] = @Localizer["title"];
}
<link rel="stylesheet" href="~/css/position.css" />
<script type="text/javascript" src="~/js/galery.js" defer></script>

<header class="hederSections">
    <h2>@ViewData["Title"]</h2>
</header>
<hr />
<section class="containerPosition">
    <article class="galery">
        <div class="bigImage"><img src="~/images/Section/Mechatronic/sm21.jpg" alt="Duze zdjecie"></div>

        <div class="miniImg">
            <div><a href="~/images/Section/Mechatronic/sm21.jpg" title="Obraz 1"><img src="~/images/Section/Mechatronic/sm21.jpg"></a></div>
            <div><a href="~/images/Section/Mechatronic/sm22.jpg" title="Obraz 2"><img src="~/images/Section/Mechatronic/sm22.jpg"></a></div>
            <div><a href="~/images/Section/Mechatronic/sm23.jpg" title="Obraz 3"><img src="~/images/Section/Mechatronic/sm23.jpg"></a></div>
            <div><a href="~/images/Section/Mechatronic/sm24.jpg" title="Obraz 4"><img src="~/images/Section/Mechatronic/sm24.jpg"></a></div>
            <div><a href="~/images/Section/Mechatronic/sm25.jpg" title="Obraz 5"><img src="~/images/Section/Mechatronic/sm25.jpg"></a></div>
        </div>
    </article>

    <article class="description">
        @Localizer["description"]
    </article>

    <article class="download">
        <heder class="DownloadHeder">
            <h3>@Localizer["download"]</h3>
        </heder>
        <hr />
        <ul>
            <li><a href="~/doc/Mechatronic/sm2.zip"><img src="~/images/Section/iconPDF.png" />@Localizer["doc1"]</a></li>
        </ul>
    </article>

</section>
