@{
    ViewData["Title"] = "Strona główna";
}

<link rel="stylesheet" href="~/css/home.css" />

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/Home/baner1.jpg" alt="Labolatorium D 109" class="img-responsive" />
            <h1 class="hederBaner carousel-caption">Labolatorium D 109</h1>
            <div class="carousel-caption" role="option">
                <p class="footerBaner">
                    Tu szkolimy przyszłych inżynierów!
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Home/baner2.jpg" alt="Roboty" class="img-responsive" />
            <h1 class="hederBaner carousel-caption">Roboty</h1>
            <div class="carousel-caption" role="option">
                <p class="footerBaner">
                    To standard w dzisiejszym przemyśle, poznaj je razem znami i naucz się projektować własne.
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Home/baner3.jpg" alt="Industrie 4.0" class="img-responsive" />
            <h1 class="hederBaner carousel-caption">Industrie 4.0</h1>
            <div class="carousel-caption" role="option">
                <p class="footerBaner">
                    Inteligentne linie produkcyjne są domeną przemysłu 4.0.
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Home/baner4.jpg" alt="Sterowniki" class="img-responsive" />
            <h1 class="hederBaner carousel-caption">Sterowniki</h1>
            <div class="carousel-caption" role="option">
                <p class="footerBaner">
                    Nowoczesne sterowniki PLC dają nam praktycznie nie ograniczone możliwości. 
                    <a class="btn btn-default" href="#">
                        Więcej
                    </a>
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<header>
    <h2>Laboratorium D 109</h2>
</header>
<section>
    Krótki opis labolatorium ...
</section>
